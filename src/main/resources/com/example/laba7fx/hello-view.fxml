<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.control.cell.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" minHeight="720.0" minWidth="1400.0" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.laba7fx.Controller">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Label fx:id="correctLabel" minHeight="18.0" minWidth="90.0" />
   <Button fx:id="button" mnemonicParsing="false" onAction="#loadTable" prefHeight="26.0" prefWidth="356.0" text="Загрузить данные в таблицу" />
    <TableView fx:id="tableId" maxWidth="1980.0" minHeight="720.0" minWidth="1280.0" VBox.vgrow="ALWAYS">
        <columns>
            <TableColumn fx:id="quoteId" maxWidth="-1.0" minWidth="50.0" prefWidth="-1.0" text="№">
                <cellValueFactory>
                    <PropertyValueFactory property="quoteId" />
                </cellValueFactory>
            </TableColumn>
            <TableColumn fx:id="quote" maxWidth="1980.0" minWidth="608.9999465942383" prefWidth="1069.8000106811523" text="Quote">
                <cellValueFactory>
                    <PropertyValueFactory property="quote" />
                </cellValueFactory>
            </TableColumn>
            <TableColumn fx:id="author" maxWidth="700.0" minWidth="65.60009765625" prefWidth="119.800048828125" text="Author">
                <cellValueFactory>
                    <PropertyValueFactory property="author" />
                </cellValueFactory>
            </TableColumn>
            <TableColumn fx:id="series" maxWidth="700.0" minWidth="77.0" prefWidth="113.4000244140625" text="Series">
                <cellValueFactory>
                    <PropertyValueFactory property="series" />
                </cellValueFactory>
            </TableColumn>
        </columns>
      <VBox.margin>
         <Insets />
      </VBox.margin>
    </TableView>
    <opaqueInsets>
        <Insets />
    </opaqueInsets>
</VBox>
